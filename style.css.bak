/* html5doctor.com Reset v1.6.1 -http://cssreset.com */
html,body,div,span,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,abbr,address,cite,code,del,dfn,em,img,ins,kbd,q,samp,small,strong,sub,sup,var,b,i,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,figcaption,figure,footer,header,hgroup,menu,nav,section,summary,time,mark,audio,video {
    margin: 0;
    padding: 0;
    border: 0;
    outline: 0;
    font-size: 100%;
    background: transparent;
}
body {
    line-height: 1;
    width: 100%;
    height: 100%;
}
article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section {
    display: block;
}
nav ul {
    list-style: none;
}
blockquote,q {
    quotes: none;
}
blockquote:before,blockquote:after,q:before,q:after {
    content: none;
}
input,select {
    vertical-align: middle;
}
body {
    background: #f4f4f4;
}
h2 {
    font-size: 2em;
    color: #1c3f94;
    margin-bottom: 20px;
}
/* ==== Header ==== */
header {
    display: block;
    margin: 0 40px;
}
#logo {
    display: block;
    line-height: 80px;
    margin-top: 20px;
    position: absolute;
}
#logo img {
    float: left;
    width: 75px;
    height: 80px;
}
#logo h1 {
    font-family: 'calibri';
    font-weight: normal;
    font-size: 5em;
    color: #1c3f94;
    float: left;
}
header nav {
    position: absolute;
    right: 40px;
}
header a {
    color: white;
    text-decoration: none;
}
header nav li {
    position: relative;
    top: -5px;
    display: inline-block;
    background: white;
    padding: 10px 30px 20px 30px;
    border-radius: 0 0 10px 10px;
    color: #fff;
    font-family: 'calibri';
    font-size: 22px;
    margin-left: 10px;
    height: 30px;
}
header nav ul li:nth-child(1),header nav ul ul:nth-child(2) {
    background: #ef4135;
}
header nav ul li:nth-child(2) {
    background: #00693c;
}
header nav ul li:nth-child(3) {
    background: #1c3f94;
    top: 0px;
}
button {
    background: none;
    border: none;
    font-size: 22px;
    color: white;
    display: inline-block;
}
header nav ul ul {
    display: none;
    position: absolute;
    top: 100%;
    margin-left: -6px;
    z-index:  999;
}
header nav ul li:hover ul {
    display: block;
}
header ul ul li {
    display: list-item;
    top: 100%;
    margin: 0;
    padding: 10px 20px;
    background: #ef4135 !important;
    border-top:  1px solid #FF756B;
    border-radius: 0;
    font-size: 0.8em;
}
header ul ul li:hover {
    background: #1c3f94 !important;
}
header h2 {
    position: absolute;
    right: 40px;
    top: 80px;
    font-family: 'Arial';
}
/* ==== Footer ==== */
footer {
    display: block;
    float: right;
    margin-right: 40px;
}
/* ==== DASHBOARD ==== */
#dashboard {
    height: 800px;
}
#left {
    float: left;
    width: 50%;
}
#messageboard {
    width: 50%;
    background: #fff;
    height: 800px;
    display: inline-block;
    margin: 0;
}
#cirkel {
    display: inline-block;
    background: url('img/cirkel.png') right;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    background-size: cover;
    width: 50%;
    height: 800px;
    margin-left: -4px;
}
#dashnav {
    padding-top: 75px;
    float: left;
    margin-left: -60px;
}
#dashnav li {
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=70)";
    filter: alpha(opacity=70);
    opacity: 0.7;
    -webkit-transition: opacity 0.4s;
    -moz-transition: opacity 0.4s;
    -o-transition: opacity 0.4s;
    -ms-transition: opacity 0.4s;
    transition: opacity 0.4s;
}
#dashnav li:hover {
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
    filter: alpha(opacity=100);
    opacity: 1;
}
#dashnav li:nth-child(2),#dashnav li:nth-child(4) {
    margin-left: 50px;
}
#dashnav li:nth-child(3) {
    margin-left: 70px;
}
/* ==== Pages ==== */
#subnav {
    background: url('img/halvecirkel.png') no-repeat;
    -webkit-background-size: contain;
    -moz-background-size: contain;
    background-size: contain;
    width: 30%;
    height: 800px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -ms-box-sizing: border-box;
    box-sizing: border-box;
    float: left;
}
#subnav img {
    margin: 320px 0 0 20px;
    float: left;
}
#subnav ul {
    list-style: none;
    float: left;
    margin-top: 320px;
    margin-left: 50px;
}
#subnav li:before {
    content: url('img/menu_button.png');
    width: 50px;
    height: 50px;
}
#subnav li a {
    text-decoration: none;
    color: #308dc7;
    font-family: 'Calibri';
    position: relative;
    top: -22px;
}
.contentarea {
    width: 70%;
    /*min-height: 700px;
    height: 100%;*/
    margin-top: 100px;
    border-radius: 20px;
    border: 3px solid #e5e5e5;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -ms-box-sizing: border-box;
    box-sizing: border-box;
    background: #fff;
    float: right;
    padding: 20px;
    font-family: 'Calibri';
}
/* ==== Ticketformulier ==== */
.knopje {
    background-color: #d14836;
    background-image: -webkit-linear-gradient(top,#dd4b39,#d14836);
    background-image: -moz-linear-gradient(top,#dd4b39,#d14836);
    background-image: -o-linear-gradient(top,#dd4b39,#d14836);
    background-image: -ms-linear-gradient(top,#dd4b39,#d14836);
    background-image: linear-gradient(top,#dd4b39,#d14836);
    border: 1px solid #d34230;
    border-radius: 5px !important;
    color: white;
    font-size: 1em;
    padding: 4px 6px;
}
.knopje:hover {
    cursor: pointer;
    background: #e35441;
}
.formulier input,select,textarea {
    font-family: 'calibri';
    font-size: 1.2em;
    height: 40px;
    width: 250px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -ms-box-sizing: border-box;
    box-sizing: border-box;
    margin-bottom: 10px;
    margin-top: 5px;
    padding-left: 5px;
    border-radius: 3px;
    border: 1px solid #e0e0e0;
}
.formulier input:focus,select:focus,textarea:focus {
    outline: none;
    border: 1px solid #d14836;
}
.formulier textarea {
    height: 80px;
}
.formulier label {
    font-size: 1.2em;
}
.formulier option {
    border: 1px solid #d14836;
}
#result {
    float: left;
    margin-left: 40px;
    margin-top: 40px;
    padding: 10px;
    border: 2px solid #499b53;
    border-radius: 15px;
    color: #6a6a6a;
    background-color: #DFF2BF;
}
#bad_result {
    float: left;
    margin-left: 40px;
    margin-top: 40px;
    padding: 10px;
    border: 2px solid #da5151;
    border-radius: 15px;
    color: #ffffff;
    background-color: #da5151;
}
/* ==== TicketOverzicht ==== */
.tabel {
    border-collapse: collapse;
    width: 100%;
}
.tabel th {
    background: #1c3f94;
    color: white;
    padding: 10px 15px;
    border: 1px solid #123383;
    width: 14.28%;
}
.tabel td {
    border: 1px solid #d6d6d6;
    text-align: center;
    width: 14.28%;
    padding: 10px 5px;
}
.tabel tr {
    background: #fff;
}
.tabel tr:nth-child(odd) {
    background: #f0f0f0;
}
/* ==== Login pagina ==== */
#loginwrap {
    position: absolute;
    background: #fff;
    width: 300px;
    padding: 0 25px;
    left: 50%;
    top: 50%;
    margin-left: -150px;
    -webkit-transform: translateY(-50%);
-moz-transform: translateY(-50%);
-o-transform: translateY(-50%);
-ms-transform: translateY(-50%);
transform: translateY(-50%);
    border-radius: 20px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -ms-box-sizing: border-box;
    box-sizing: border-box;
    border: 1px solid #b2b3b2;
}
#loginwrap h2 {
    padding: 10px 0;
    margin-bottom:  0px;
    font-family: 'Calibri';
}